{
  "name": "a-yasui/laravel-chatwork",
  "description": "Laravel notification driver for Chatwork.",
  "keywords": [
    "laravel",
    "chatwork",
    "notification"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "yokuru",
      "email": "takashi.yokubo@gmail.com"
    },
    {
      "name": "a.yasui",
      "email": "a.yasui@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "illuminate/notifications": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "ATYasu\\Chatwork\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ATYasu\\Chatwork\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ATYasu\\Chatwork\\ServiceProvider"
      ]
    }
  }
}
